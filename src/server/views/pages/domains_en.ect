        <div class="promotion text-center text-white text-bold">
          <a href="/download">
            <img src="<%= @__asset('img/icons/apple.png') %>" alt="iOS" width="43" height="52">
            <span><%- @__('DOWNLOAD_IOS_PROFILE') %></span>
          </a>
        </div>
        <div class="promotion text-center text-white text-bold">
          <a href="/download">
            <img src="<%= @__asset('img/icons/android.png') %>" alt="android" width="48" height="57">
            <span><%- @__('DOWNLOAD_ANDROID_PROFILE') %></span>
