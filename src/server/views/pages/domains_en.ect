<% extend '_flow' %>

<% block 'head_end': %>

<link rel="stylesheet" href="<%= @__asset('domains.css') %>">

<% end %>

<% block 'page_flow': %>

<div class="container">
  <h1 class="page-title text-center text-upper"><%- @__('DEVELOPMENT_DOMAINS') %></h1>
</div>
<div class="container domains">
  <div class="domain">
    <h2 class="heading text-upper text-blue"><%- @__('MOBILE_APPLICATION_DEVELOPMENT') %></h2>
    <div class="explain">
      <h3 class="sub-heading text-blue"><%- @__('MOBILE_APPLICATION_DEVELOPMENT_DESC') %></h3>
      <p><%- @__('MOBILE_APPLICATION_DEVELOPMENT_EXPLAIN') %></p>
